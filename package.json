{
  "name": "assistant",
  "version": "0.1.0",
  "description": "Motif assistant script for web-based technical support",
  "main": "lib/assistant.js",
  "scripts": {
    "build": "webpack --env dev && webpack --env build && npm run test",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "mocha --require babel-core/register --colors ./test/*.spec.js",
    "test:watch": "mocha --require babel-core/register --colors -w ./test/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/usermotif/assistant.js"
  },
  "keywords": [
    "motif",
    "customer",
    "support",
    "sdk"
  ],
  "author": "Motif Technologies",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/usermotif/assistant.js/issues"
  },
  "homepage": "https://github.com/usermotif/assistant.js",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "eslint": "^4.13.1",
    "eslint-loader": "^1.9.0",
    "mocha": "^4.0.1",
    "webpack": "^3.10.0",
    "yargs": "^10.0.3"
  }
}
